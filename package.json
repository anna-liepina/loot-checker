{
  "name": "loot-checker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "papaparse": "^5.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "node-sass": "^4.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*/**/**.js?(x)",
      "!src/docs/**",
      "!src/*/**index.js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
